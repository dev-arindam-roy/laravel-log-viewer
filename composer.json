{
    "name": "creative-syntax/laravel-logviewer",
    "description": "A simple laravel package for log viewer",
    "type": "laravel-package",
    "license": "MIT",
    "keywords": [
        "log",
        "laravel-log-viewer",
        "laravel-log-reader",
        "log-reader",
        "log-viewer",
        "logging",
        "laravel"
    ],
    "authors": [
        {
            "name": "Arindam Roy",
            "email": "arindam.roy.developer@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*|5.*|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "CreativeSyntax\\LogViewer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CreativeSyntax\\LogViewer\\CreativeSyntaxLogViewer"
            ]
        }
    }
}
